import numpy as np 
from numpy.random import rand, randint

array_1d = np.array([1,2,3,4,5])
print("arrary 1차원:\n",array_1d)
print("arrary 1차원(array_1d)의 클래스:\n",type(array_1d))
print()
array_2d = np.array([[1,2,3,4,5],[2,3,4,5,6]])
print("arrary 2차원:\n",array_2d)
print("arrary 2차원(array_2d)의 클래스:\n",type(array_2d))
print()
array_3d = np.array([[[1,2],[3,4]],[[5,6],[7,8]]])
print("arrary 3차원:\n",array_3d)
print("arrary 3차원(array_3d)의 클래스:\n",type(array_3d))
